{
  "version": 2,
  "builds": [
    {
      "src": "api/api.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/api.js"
    },
    {
      "src": "/instagram",
      "dest": "/api/api.js"
    }
  ],
  "functions": {
    "api/api.js": {
      "maxDuration": 60
    }
  }
}
